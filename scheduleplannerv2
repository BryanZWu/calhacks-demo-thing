class Semester{
    constructor(season, year, number, courses = []){
        this._season = season;
        this._year = year;
	this._number = number
        this._courses = courses;
    }
    get season(){
    	return this._season;
    }
    set season(s){
        this._season = s;
    }
    get year(){
    	return this._year;
    }
    set year(y){
        this._year = y;
    }
    get number(){
    	return this._number;
    }
    set number(n){
	this._number = n;
    }
    get courses(){
    	return this._courses.toString();
    }
    set courses(c){
        this._courses = [c];
    }
    add_course(new_course){
        this._courses.push(new_course);
    }
    delete_course(course_name){
        var index = this._courses.indexOf(course_name);
	this._courses.splice(index, index+1);
    }
}

class Course{
    constructor(name, units=4, semester=null){
        this._name = name;
        this._units = units;
	this._semester = semester
    }
    get name(){
    	return this._name;
    }
    set name(n){
        this._name = n;
    }
    get units(){
    	return this._units;
    }
    set units(u){
        this._units = u;
    }
    get semester(){
    	return this._semester;
    }
    set semester(u){
        this._semester = semester;
    }
    toString(){
        return this._name
    }

}

var math1a = new Course("Math 1A");
var math1b = new Course("Math 1B");
var math53 = new Course("Math 53");
var physicsa = new Course("Physics 7A or 5A");
var physicsb = new Course("Physics 7B or 5B+5BL");
var cs70 = new Course("CS 70");
var natsci = new Course("Natural Science Other");
var eecsdesign1 = new Course("EECS Design 1");
var eecsdesign2 = new Course("EECS Design 2");
var eecsdesign3 = new Course("EECS Design 3");
var eecsdesign4 = new Course("EECS Design 4");
 

class Requirements{
    constructor(requirements = {}){
	this._requirements = requirements;
    }
    get requirements(){
    	return this._requirements;
    }
    set requirements(d){
        this._requirements = d
    }
}

class University_req extends Requirements{
    constructor(){
        super({"Math 1A": math1a, "Math 1B": math1b, "Math 53": math53});
    }	
}

class EECS_req extends University_req{
    constructor(){
        super();
	var EECS_dict = {"CS 70": cs70, "Physics 7A or Physics 5A": physicsa, "Physics 7B or Physics 5B+5BL": physicsb, "Natural Science Other": natsci, "EECS Design 1": eecsdesign1, "EECS Design 2": eecsdesign2, "EECS Design 3": eecsdesign3, "EECS Design 4": eecsdesign4}
        for (var key in EECS_dict){
	    this.requirements[key] = EECS_dict[key];
        }
    }
}

var course_list = [math1a, math1b, math53, cs70, physicsa, physicsb, natsci, eecsdesign1, eecsdesign2, eecsdesign3, eecsdesign4]

var semester1 = new Semester("Fall", 2019, 1)
var semester2 = new Semester("Spring", 2020, 2)
var semester3 = new Semester("Fall", 2020, 3)
var semester4 = new Semester("Spring", 2021, 4)
var semester5 = new Semester("Fall", 2021, 5)
var semester6 = new Semester("Spring", 2022, 6)
var semester7 = new Semester("Fall", 2022, 7)
var semester8 = new Semester("Spring", 2023, 8)

var math_arr = [];
for (var key in new EECS_req().requirements){
	    math_arr += [key];
        }
math_arr.splice(3,7);
console.log(math_arr);

