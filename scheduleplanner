class Semester{
    constructor(season, year, courses = []){
        this._season = season;
        this._year = year;
        this._courses = courses;
    }
    get season(){
    	return this._season;
    }
    set season(s){
        this._season = s;
    }
    get year(){
    	return this._year;
    }
    set year(y){
        this._year = y;
    }
    get courses(){
    	return this._courses.toString();
    }
    set courses(c){
        this._courses = [c];
    }
    add_course(new_course){
        this._courses.push(new_course)
    }
    delete_course(course_name){
        index = this._courses.findIndex(course_name)
	this._courses.splice(index, index)
    }
}

class Course{
    constructor(name, units=4){
        this._name = name;
        this._units = units;
    }
    get name(){
    	return this._name;
    }
    set name(n){
        this._name = n;
    }
    get units(){
    	return this._units;
    }
    set units(u){
        this._units = u;
    }
    toString(){
        return this._name
    }

}
var course = new Course("EE16a");
console.log(course.name);
console.log(course.units);

var semester = new Semester("Fall", 2019, [course, new Course("CS61a")]);
console.log(semester.year);
console.log(semester.courses);

semester.add_course(new Course("Math 53"));
console.log(semester.courses);

semester.delete_course(course);
console.log(semester.courses);



